<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.shu.crocodile.ArtistActivity"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <ImageButton
        android:id="@+id/imageButton_white"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginBottom="16dp"
        android:backgroundTint="@android:color/background_light"
        android:onClick="onClick_white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/imageButton_blueBlue"
        app:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintBottom_creator="1" />

    <ImageButton
        android:id="@+id/imageButton_purple"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:backgroundTint="@android:color/holo_purple"
        android:onClick="onClick_purple"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/imageButton_orange"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/imageButton_white"
        android:layout_marginBottom="17dp" />

    <ImageButton
        android:id="@+id/imageButton_black"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:backgroundTint="@color/black"
        android:onClick="onClick_black"
        app:layout_constraintRight_toLeftOf="@+id/imageButton_brown"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton_brown" />

    <ImageButton
        android:id="@+id/imageButton_brown"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:backgroundTint="?android:attr/colorActivatedHighlight"
        android:onClick="onClick_brown"
        app:layout_constraintRight_toLeftOf="@+id/imageButton_redDark"
        app:layout_constraintLeft_toRightOf="@+id/imageButton_black"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton_redDark" />

    <ImageButton
        android:id="@+id/imageButton_redDark"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:backgroundTint="@android:color/holo_red_dark"
        android:onClick="onClick_redDark"
        app:layout_constraintRight_toLeftOf="@+id/imageButton_red"
        app:layout_constraintLeft_toRightOf="@+id/imageButton_brown"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton_red" />

    <ImageButton
        android:id="@+id/imageButton_orange"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:backgroundTint="@android:color/holo_orange_light"
        android:onClick="onClick_orange"
        app:layout_constraintRight_toLeftOf="@+id/imageButton_purple"
        app:layout_constraintLeft_toRightOf="@+id/imageButton_pink"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton_purple" />

    <ImageButton
        android:id="@+id/imageButton_pink"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:backgroundTint="?attr/colorAccent"
        android:onClick="onClick_pink"
        app:layout_constraintRight_toLeftOf="@+id/imageButton_orange"
        app:layout_constraintLeft_toRightOf="@+id/imageButton_red"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton_orange" />

    <ImageButton
        android:id="@+id/imageButton_blueLigth"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:backgroundTint="@android:color/holo_blue_bright"
        android:onClick="onClick_blueLigth"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton_blue"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageButton_blue"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <ImageButton
        android:id="@+id/imageButton_greenLigth"
        android:layout_width="35dp"
        android:layout_height="0dp"
        android:backgroundTint="@android:color/holo_green_light"
        android:onClick="onClick_greenLigth"
        app:layout_constraintRight_toLeftOf="@+id/imageButton_greenDark"
        app:layout_constraintLeft_toRightOf="@+id/imageButton_yellow"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton_yellow"
        app:layout_constraintTop_toTopOf="@+id/imageButton_yellow" />

    <ImageButton
        android:id="@+id/imageButton_greenDark"
        android:layout_width="35dp"
        android:layout_height="0dp"
        android:backgroundTint="@android:color/holo_green_dark"
        android:onClick="onClick_greenDark"
        app:layout_constraintRight_toLeftOf="@+id/imageButton_blueLigth"
        app:layout_constraintLeft_toRightOf="@+id/imageButton_greenLigth"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton_greenLigth"
        app:layout_constraintTop_toTopOf="@+id/imageButton_greenLigth" />

    <ImageButton
        android:id="@+id/imageButton_red"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:backgroundTint="@android:color/holo_red_light"
        android:onClick="onClick_red"
        app:layout_constraintRight_toLeftOf="@+id/imageButton_pink"
        app:layout_constraintLeft_toRightOf="@+id/imageButton_redDark"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton_pink" />

    <ImageButton
        android:id="@+id/imageButton_blue"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:backgroundTint="@android:color/holo_blue_dark"
        android:onClick="onClick_blue"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton_blueBlue"
        app:layout_constraintLeft_toRightOf="@+id/imageButton_blueLigth"
        app:layout_constraintRight_toLeftOf="@+id/imageButton_blueBlue"
        app:layout_constraintTop_toTopOf="@+id/imageButton_blueBlue"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintTop_creator="1" />

    <ImageButton
        android:id="@+id/imageButton_blueBlue"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:backgroundTint="@android:color/holo_blue_light"
        android:onClick="onClick_blueBlue"
        app:layout_constraintBottom_toBottomOf="@+id/imageButton_white"
        app:layout_constraintLeft_toRightOf="@+id/imageButton_blue"
        app:layout_constraintRight_toLeftOf="@+id/imageButton_white"
        app:layout_constraintTop_toTopOf="@+id/imageButton_white"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintTop_creator="1" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:srcCompat="@android:drawable/screen_background_light"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/imageButton_purple"
        android:layout_marginStart="22dp"
        android:layout_marginEnd="22dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="12dp"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="20dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginLeft="22dp"
        android:layout_marginRight="22dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="СЛОВО"
        android:textAppearance="@style/TextAppearance.AppCompat.Headline"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="22dp"
        android:layout_marginTop="7dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/textView"
        android:layout_marginLeft="22dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Нарисуй :"
        android:textAppearance="@style/TextAppearance.AppCompat.Headline"
        tools:layout_constraintTop_creator="1"
        android:layout_marginStart="22dp"
        android:layout_marginTop="7dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="22dp" />

    <ImageButton
        android:id="@+id/imageButton_yellow"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:backgroundTint="?android:attr/colorPressedHighlight"
        android:onClick="onClick_yellow"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton_black"
        app:layout_constraintRight_toLeftOf="@+id/imageButton_greenLigth"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintBottom_creator="1"
        android:layout_marginTop="17dp"
        android:layout_marginBottom="16dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:onClick="onClick_clear"
        android:background="@android:drawable/btn_dialog"
        tools:layout_editor_absoluteX="328dp"
        tools:layout_editor_absoluteY="8dp" />

</android.support.constraint.ConstraintLayout>
